			<module>juzu-ajax</module>
		</depends>
		<depends>
			<module>jquery</module>
		</depends>
		<depends>
			<module>jQueryDataTable</module>
		</depends>
	</module>
	<module>
		<name>jQueryDataTable</name>
		<script>
			<path>/js/extensiongenerator/jquery.dataTables.js</path>
