<?xml version="1.0" encoding="UTF-8"?>
<bundle>
	<UITabForm>
		<label>
			<pushContent>Push Content</pushContent>
		</label>
	</UITabForm>
	<UIActionBar>
		<tooltip>
			<PushContent>Push Content</PushContent>
		</tooltip>
	</UIActionBar>
	<PushContent>
		<msg>
			<unableToConnect>Unable to connect to server</unableToConnect>
			<synchronizationError>An error occurred while synchronizing content</synchronizationError>
			<targetServerMandatory>Target Server is mandatory</targetServerMandatory>
			<userNameMandatory>Login is mandatory</userNameMandatory>
			<passwordMandatory>Password is mandatory</passwordMandatory>
			<synchronizationDone>Synchronization done successfully</synchronizationDone>
		</msg>
		<state>
			<not_found_source>Not found on source</not_found_source>
			<not_found_target>Not found on target</not_found_target>
			<modified_source>Modified on source</modified_source>
			<modified_target>Modified on target</modified_target>
			<same>Same</same>
			<unkown>Unknown</unkown>
			<all>All</all>
		</state>
	</PushContent>
	<PushContentPopupComponent>
		<label>
			<targetServer>Target Server</targetServer>
			<username>Login</username>
			<password>Password</password>
			<synchronizationNode>Node</synchronizationNode>
			<publishOnTarget>Publish on target server</publishOnTarget>
      <currentContent>Current Content</currentContent>
      <currentSubContents>Sub contents of the current content</currentSubContents>
		</label>
		<action>
			<Select>Edit selection</Select>
			<Push>Push</Push>
			<Close>Close</Close>
			<title>
				<delete>Delete</delete>
			</title>
		</action>
		<header>
			<title>Title</title>
			<path>Path</path>
			<action>Action</action>
		</header>
	</PushContentPopupComponent>
	<SelectNodesPopupWindow>
		<title>
			<SelectNodesPopupComponent>Select Nodes to synchronize</SelectNodesPopupComponent>
		</title>
	</SelectNodesPopupWindow>
	<SelectNodesPopupComponent>
		<header>
			<title>Title</title>
			<path>Path</path>
			<lastModifierUserName>Modified by</lastModifierUserName>
			<sourceModificationDate>Modified (source)</sourceModificationDate>
			<targetModificationDate>Modified (target)</targetModificationDate>
			<stateLocalized>State</stateLocalized>
			<action>Action</action>
		</header>
		<action>
			<title>
				<Select>Select content</Select>
			</title>
		</action>
		<label>
			<contentState>State</contentState>
			<search>Search</search>
			<filter>Filter</filter>
			<modifiedAfter>Modified here after</modifiedAfter>
			<publishedContentOnly>Published content only</publishedContentOnly>
			<selectContent>Filter results</selectContent>
			<selectedContent>Selected contents</selectedContent>
			<selectAll>Select all</selectAll>
			<deleteAll>Delete all</deleteAll>
		</label>
	</SelectNodesPopupComponent>
	<UIPopupWindow>
		<title>
			<PushContentPopupComponent>Push Content</PushContentPopupComponent>
		</title>
	</UIPopupWindow>
</bundle>