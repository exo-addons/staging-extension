<?xml version="1.0" encoding="UTF-8"?>
<bundle>
	<UITabForm>
		<label>
			<pushContent>Push Content</pushContent>
		</label>
	</UITabForm>
	<UIActionBar>
		<tooltip>
			<PushContent>Push Content</PushContent>
		</tooltip>
	</UIActionBar>
	<PushContent>
		<msg>
			<unableToConnect>Unable to connect to server</unableToConnect>
			<synchronizationError>An error occurred while synchronizing content. Please contact your administrator.</synchronizationError>
			<targetServerMandatory>Target Server is mandatory</targetServerMandatory>
			<userNameMandatory>Login is mandatory</userNameMandatory>
			<passwordMandatory>Password is mandatory</passwordMandatory>
			<synchronizationDone>Synchronization done successfully</synchronizationDone>
			<synchronizationInProgress>Synchronization in progress ...</synchronizationInProgress>
		</msg>
		<state>
			<not_found_source>New (on target)</not_found_source>
			<not_found_target>New (here)</not_found_target>
			<modified_source>Modified (source)</modified_source>
			<modified_target>Modified (target)</modified_target>
			<same>Same</same>
			<unkown>Unknown</unkown>
			<all>All</all>
		</state>
		<action>
			<not_found_source>Delete from target</not_found_source>
			<not_found_target>Add to target</not_found_target>
			<modified_source>Update on target</modified_source>
			<modified_target>Force update on target</modified_target>
			<same>Update on target</same>
			<unkown>Unknown</unkown>
		</action>
	</PushContent>
	<PushContentPopupComponent>
		<label>
			<targetServer>Target Server</targetServer>
			<username>Login</username>
			<password>Password</password>
			<synchronizationNode>Node</synchronizationNode>
			<publishOnTarget>Publish on target server</publishOnTarget>
			<currentContent>Current Content</currentContent>
			<currentSubContents>Sub contents of the current content</currentSubContents>
		</label>
		<action>
			<Select>Modify selection</Select>
			<Push>Push</Push>
			<Close>Close</Close>
			<title>
				<delete>Delete</delete>
			</title>
		</action>
		<header>
			<title>Title</title>
			<path>Path</path>
			<action>Action</action>
		</header>
	</PushContentPopupComponent>
	<SelectNodesComponent>
		<header>
			<title>Title</title>
			<path>Path</path>
			<lastModifierUserName>Modified by</lastModifierUserName>
			<published>Published here</published>
			<sourceModificationDate>Modified (here)</sourceModificationDate>
			<targetModificationDate>Modified (target)</targetModificationDate>
			<stateLocalized>State</stateLocalized>
			<actionLocalized>Action</actionLocalized>
			<action>Action</action>
		</header>
		<label>
			<published>Published</published>
			<notPublished>Not published</notPublished>
			<selectContent>Contents</selectContent>
			<selectedContent>Selected contents</selectedContent>
			<selectAll>Select all</selectAll>
			<deleteAll>Delete all</deleteAll>
		</label>
	</SelectNodesComponent>
	<UIPopupWindow>
		<title>
			<PushContentPopupComponent>Push Content</PushContentPopupComponent>
		</title>
	</UIPopupWindow>
</bundle>