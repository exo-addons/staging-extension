			<targetServerMandatory>Le choix du serveur cible est obligatoire</targetServerMandatory>
			<userNameMandatory>Le nom de l'utilisateur est obligatoire</userNameMandatory>
			<passwordMandatory>Le mot de passe est obligatoire</passwordMandatory>
			<synchronizationDone>La synhronisation est terminée avec succès</synchronizationDone>
		</msg>
		<state>
			<not_found_source>Manquant ici</not_found_source>
			<not_found_target>Manquant sur le serveur cible</not_found_target>
			<modified_source>Modifié ici</modified_source>
			<modified_target>Modifié sur le serveur cible</modified_target>
			<same>Egaux</same>
			<unkown>Inconnu</unkown>
			<all>Tout</all>
		</state>
	</PushContent>
	<PushContentPopupComponent>
		<label>
			<targetServer>Serveur cible</targetServer>
			<username>Login</username>
			<password>Mot de passe</password>
			<synchronizationNode>Noeud</synchronizationNode>
			<publishOnTarget>Publier sur le serveur cible</publishOnTarget>
      <currentContent>Contenu courant</currentContent>
      <currentSubContents>Sous-contenus du contenu courant</currentSubContents>
		</label>
		<action>
			<Select>Sélectionner manuellement les contenus</Select>
			<Push>Synchroniser</Push>
			<Close>Fermer</Close>
			<title>
				<delete>Supprimer</delete>
			</title>
		</action>
		<header>
			<title>Titre</title>
			<path>Chemin</path>
			<action>Action</action>
		</header>
	</PushContentPopupComponent>
	<SelectNodesPopupWindow>
		<title>
			<SelectNodesPopupComponent>Sélectionner manuellement les contenus à synchroniser</SelectNodesPopupComponent>
		</title>
	</SelectNodesPopupWindow>
	<SelectNodesPopupComponent>
		<header>
			<title>Titre</title>
			<path>Chemin</path>
			<lastModifierUserName>Modifié par</lastModifierUserName>
			<sourceModificationDate>Modifié le (ici)</sourceModificationDate>
			<targetModificationDate>Modifié le (distant)</targetModificationDate>
			<stateLocalized>Etat</stateLocalized>
			<action>Action</action>
		</header>
		<action>
			<title>
				<Select>Sélectionner un contenu</Select>
			</title>
		</action>
		<label>
			<contentState>Etat</contentState>
			<search>Rechercher</search>
			<filter>Filtrer</filter>
			<modifiedAfter>Modifié sur ce serveur, après</modifiedAfter>
